#bridge_name[zoom>=15][zoom<=20]{
	text-name:'[bridge_name]';
    text-face-name:@sans_italic;
    text-halo-radius: 1;
    text-fill: #333;
    text-halo-fill: #fff;

	text-placement: point;
	text-placement-type: simple;
	text-placements: "N, NE, NW, S, SE, SW, E, W";
	text-avoid-edges: true;
	text-dx: 5; text-dy: 5;
}
